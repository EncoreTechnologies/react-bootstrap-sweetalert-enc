{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": true,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "no-empty": false,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "examples",
      "*.md"
    ]
  }
}
